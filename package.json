{
  "name": "mmorpg-telegram-bot",
  "version": "1.0.0",
  "description": "MMO RPG Telegram Bot built with TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:bot": "jest tests/bot",
    "test:api": "jest tests/api",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --write src tests",
    "format:check": "prettier --check src tests",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prestart": "npm run build",
    "docker:build": "docker build -t mmorpg-bot .",
    "docker:run": "docker run -p 3000:3000 mmorpg-bot"
  },
  "keywords": [
    "telegram",
    "bot",
    "mmorpg",
    "rpg",
    "game",
    "typescript",
    "nodejs"
  ],
  "author": "MMO RPG Bot Team",
  "license": "MIT",
  "dependencies": {
    "telegraf": "^4.15.6",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "redis": "^4.6.10",
    "ioredis": "^5.3.2",
    "prisma": "^5.7.1",
    "@prisma/client": "^5.7.1",
    "canvas": "^2.11.2",
    "sharp": "^0.33.1",
    "uuid": "^9.0.1",
    "crypto": "^1.0.1",
    "axios": "^1.6.2",
    "node-cron": "^3.0.3",
    "multer": "^1.4.5-lts.1",
    "express-fileupload": "^1.4.3",
    "joi": "^17.11.0",
    "class-validator": "^0.14.0",
    "class-transformer": "^0.5.1",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.8.0",
    "inversify": "^6.0.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/uuid": "^9.0.7",
    "@types/multer": "^1.4.11",
    "@types/node-cron": "^3.0.11",
    "@types/jest": "^29.5.8",
    "@types/supertest": "^2.0.16",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/mmorpg-telegram-bot.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/mmorpg-telegram-bot/issues"
  },
  "homepage": "https://github.com/your-username/mmorpg-telegram-bot#readme"
}
